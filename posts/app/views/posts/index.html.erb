<h1>Here are the Posts</h1>
<% @posts.each do |post| %>
  <div>
    <h2><%=post.author%></h2><%=link_to "Read More", post_path(post) %> |
    <%= link_to "Delete Author", post_path(post), method: :delete %><hr/>
<% end %>
</div>
<br/>
<button><h2><%=link_to "Add Author", new_post_path%></h2></button>
